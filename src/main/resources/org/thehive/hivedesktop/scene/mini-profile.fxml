<VBox alignment="CENTER" prefHeight="430.0" prefWidth="408.0" styleClass="VBoxes" stylesheets="@../../../../css/style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.thehive.hivedesktop.controllers.ProfileDialogViewController">
