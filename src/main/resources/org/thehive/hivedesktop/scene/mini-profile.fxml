                    <ImageView fx:id="" fitHeight="26.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true" style="-fx-border-radius: 50; -fx-background-radius: 50;">
                        <image>
                            <Image url="@../../../../img/close.png" />
                        </image>
                    </ImageView>
                </graphic>
                <styleClass>
                    <String fx:value="transparent" />

                </styleClass>
            </Button>
        </buttons>
      </ButtonBar>
      <ImageView fx:id="imageProfile" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">

      </ImageView>
      <Pane prefHeight="12.0" prefWidth="442.0" />
      <Label fx:id="lblFullName" alignment="CENTER" prefHeight="27.0" prefWidth="239.0" text="Mustafa Tüfekçi" textFill="#373737">
