      <Label fx:id="lblUsername" prefHeight="22.0" prefWidth="266.0" text="mustafatufekci" textFill="#373737">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Label>
      <Pane prefHeight="10.0" prefWidth="378.0" />
      <Label prefHeight="22.0" prefWidth="265.0" text="Email" textFill="#b49300">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblEmail" prefHeight="22.0" prefWidth="266.0" text="mustafa.tufekci@gmail.com" textFill="#373737">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Label>
